{
  "name": "traffic-generator",
  "version": "2.0.0",
  "description": "FAANG-level traffic generator for Smart Gateway Platform",
  "type": "module",
  "main": "advanced-generator.js",
  "scripts": {
    "start": "bun run advanced-generator.js",
    "start:basic": "bun run generator.js",
    "start:native": "bun run generator-bun.js",
    "demo": "MODE=demo bun run advanced-generator.js",
    "realistic": "MODE=realistic bun run advanced-generator.js",
    "steady": "MODE=steady bun run advanced-generator.js",
    "ramp-up": "MODE=ramp-up bun run advanced-generator.js",
    "spike": "MODE=spike bun run advanced-generator.js",
    "load-1k": "bun run load-1k.js",
    "load-10k": "bun run load-10k.js",
    "load-100k": "bun run load-100k.js",
    "dev": "bun --watch advanced-generator.js"
  },
  "dependencies": {
    "axios": "^1.6.2"
  }
}

